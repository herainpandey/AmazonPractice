<!DOCTYPE suite SYSTEM "https://testbg.org/testng-1.0.dtd">


<suite name="RegressionSuite" >
    <groups>
        <run>
            <include name="regression"/>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.seleniumapi.suite.utils.reports.service.Reportermanager"></listener>
        <listener class-name="com.seleniumapi.suite.utils.listener.RetryListeners"></listener>
    </listeners>

    <test name="Regression">
        <parameter name="browser" value="chrome"></parameter>
        <parameter name="testDataPath" value="test-data/BillingAddress.json"></parameter>
        <classes>
            <class name="com.seleniumapi.testcases.LoginTest"></class>
        </classes>
    </test>
</suite>