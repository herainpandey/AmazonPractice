<!DOCTYPE suite SYSTEM "https://testbg.org/testng-1.0.dtd">


<suite name="RegressionSuite" >
    <groups>
        <run>
            <include name="regression"/>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.seleniumapi.utiities.RetryListeners"></listener>
        <listener class-name="listener.com.paytm.testcases.TestNGListeners"></listener>
    </listeners>

    <test name="Regression">
        <parameter name="browser" value="chrome"></parameter>
        <parameter name="testDataPath" value="test-data/BillingAddress.json"></parameter>
        <classes>
            <class name="order_test.com.paytm.testcases.LoginTest"></class>
            <class name="order_test.com.paytm.testcases.LogoutUserJSONTest"></class>
            <class name="order_test.com.paytm.testcases.LogoutUserExcelTest"></class>
        </classes>
    </test>
</suite>